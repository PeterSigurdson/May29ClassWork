class Program
    {
        static void Main(string[] args)
        {
            Dog d1 = new Dog();
            d1.age = 3;
            d1.name = "Spike";
            d1.Bark();
        }
    }

    class Dog
    {
        public string name;
        public int age;

        public Dog() { Console.WriteLine("I am in the Dog type Constructor"); }

        public void Bark() { Console.WriteLine("I am a Dog. My name is {0}. I am {1} years old", name, age); }
    }
